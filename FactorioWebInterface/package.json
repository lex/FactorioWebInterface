    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "process": "^0.11.10",
    "remove-files-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "typings-for-css-modules-loader": "^1.7.0",
    "util": "^0.12.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
