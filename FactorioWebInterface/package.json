{
  "name": "FactorioWebInterface",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=development",
    "build-test": "webpack --mode=development --env test",
    "watch": "webpack --mode=development --watch",
    "release": "webpack --mode=production",
    "publish": "npm run release && dotnet publish -c Release",
    "test": "npm run build-test && mocha --exit ./wwwroot/js/tests.js",
    "test-debug": "npm run build-test && mocha --exit --inspect-brk ./wwwroot/js/tests.js",
    "test-coverage": "nyc npm run test"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/testUtils/**",
      "**/*HubService.ts"
    ],
    "reporter": [
      "text"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@microsoft/signalr": "^6.0.1",
    "@microsoft/signalr-protocol-msgpack": "^6.0.1",
    "@types/jquery": "^3.5.11",
    "@types/node": "^16.11.17",
    "bulma": "0.8.0",
    "deep-equal": "^2.0.5"
  },
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/deep-equal": "^1.0.1",
    "@types/mocha": "^9.0.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.1",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
